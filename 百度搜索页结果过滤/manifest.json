{   
    "manifest_version": 2,
    "name": "baiduResult_titleScaleChange",
    "description": "baiduResult_titleScaleChange",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["*://*.baidu.com/*"],
        "js": ["jquery.min.js", "process.js","main.js"],
        "run_at": "document_start"
      }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "*://*.baidu.com/*",
        "https://ajax.googleapis.com/"
      ],
      "web_accessible_resources": [
        "*.css",
        "*.js"
      ]
}